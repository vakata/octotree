{
  "name": "Octotree-vakata",
  "version": "1.1.0",
  "manifest_version": 2,
  "author": "Ivan Bozhanov",
  "description": "Display GitHub code in tree format",
  "homepage_url": "https://github.com/vakata/octotree",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://github.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "data/octotree-vakata.css"
      ],
      "js": [
        "data/octotree-vakata.js"
      ]
    }
  ]
}